<div class="requestForm">
<%= form_for(request) do |f| %>
  <% if request.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(request.errors.count, "error") %> prohibited this request from being saved:</h2>

      <ul>
      <% request.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :schedule_id %>
    <%= f.text_field :schedule_id, :value => params[:request_schedule_id] %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.text_field :user_id, :value => params[:request_user_id] %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</div>